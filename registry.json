      "type": "registry:layout",
      "title": "App Layouts",
      "description": "Application layouts (AuthenticatedLayout for logged users, DefaultLayout for public pages)",
      "categories": ["layout"],
      "files": [
        {
          "path": "app/layouts/AuthenticatedLayout.vue",
          "type": "registry:layout"
