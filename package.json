{
  "name": "vue-blocks-registry",
  "version": "0.2.4",
  "type": "module",
  "description": "A modular system for Vue 3 projects with reusable UI components, features, and bundles",
  "author": "Jan madeyski <jan.madeyski@gmail.com>",
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui",
    "registry",
    "cli"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:cli": "tsc --project cli/tsconfig.json",
    "build:registry": "tsx scripts/build-registry.ts",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "cli": "tsx cli/src/index.ts",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "prepublishOnly": "pnpm run build:registry && pnpm run build:cli && pnpm run type-check && pnpm run lint"
  },
  "license": "MIT",
  "homepage": "https://vue-blocks-registry.dev-made.it",
  "repository": {
    "type": "git",
    "url": "https://github.com/jm-sky/vue-blocks-registry.git"
  },
  "bugs": {
    "url": "https://github.com/jm-sky/vue-blocks-registry/issues"
  },
  "files": [
    "cli/dist/**/*",
    "registry/**/*",
    "public/r/**/*",
    "registry.json",
    "README.md"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "bin": {
    "vue-blocks-registry": "./cli/dist/index.js"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.90.5",
    "@tanstack/vue-table": "^8.21.3",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "floating-vue": "^5.2.2",
    "fs-extra": "^11.3.2",
    "lucide-vue-next": "^0.546.0",
    "ora": "^9.0.0",
    "pinia": "^3.0.3",
    "prompts": "^2.4.2",
    "reka-ui": "^2.5.1",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.9",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@tsconfig/node22": "^22.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.6",
    "@types/prompts": "^2.4.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.13",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.33.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-vue": "~10.4.0",
    "favicons": "^7.2.0",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  },
  "packageManager": "pnpm@10.18.3"
}
